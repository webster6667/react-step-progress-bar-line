{
  "name": "react-step-progress-bar-line",
  "version": "1.0.0",
  "description": "progress bar with delimited sections",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "prebuild": "run-s clean",
    "build": "run-s build:types build:prod build:theme",
    "build:docs": "jsdoc2md --template ./README.hbs --files ./src/*.tsx --configure ./jsdoc2md.json > ./README.md",
    "build:types": "tsc --project tsconfig.json",
    "build:prod": "cross-env NODE_ENV=prod rollup -c",
    "build:dev": "run-s build:types && cross-env NODE_ENV=dev rollup -c --watch",
    "build:theme": "sass src/themes:dist/style/themes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webster6667/react-step-progress-bar-line.git"
  },
  "keywords": [],
  "author": "webster6667",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webster6667/react-step-progress-bar-line/issues"
  },
  "peerDependencies": {
    "@types/react": "^17.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "bem-components-connector": "^1.0.5",
    "get-array-sum": "^1.0.1",
    "react-line-progress-bar": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.1",
    "cross-env": "^7.0.3",
    "husky": "4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.0",
    "jsdoc-babel": "0.5.0",
    "jsdoc-to-markdown": "6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.48.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.39.2",
    "scss-coding-helpers": "^1.0.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0"
  },
  "homepage": "https://github.com/webster6667/react-step-progress-bar-line#readme"
